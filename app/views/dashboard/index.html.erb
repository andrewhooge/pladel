<% provide(:title, 'Dashboard') %>

<%= link_to 'Settings', edit_user_path(current_user) %>

<div class="health">
  <%= current_user.health_gpa %>
</div>

<div class="sleep">
  <%= current_user.sleep_score %>
</div>

<div class="movement">
  <%= current_user.movement_score %>
</div>

<div class="nutrition">
  <%= current_user.nutrition_score %>
</div>
